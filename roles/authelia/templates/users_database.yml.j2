---
###############################################################
#                         Users Database                      #
###############################################################

# This file can be used if you do not have an LDAP set up.

# List of users
users:
  {{ user_name }}:
    displayname: "{{ user_name }}"
    password: "{{ user_password | string | password_hash('sha512') }}"
    email: "{{ user_email }}"
    groups:
      - admins
      - dev
