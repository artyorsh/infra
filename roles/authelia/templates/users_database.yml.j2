---
###############################################################
#                         Users Database                      #
###############################################################

# This file can be used if you do not have an LDAP set up.

# List of users
users:
  {{ system_user }}:
    displayname: "{{ system_user }}"
    password: "{{ system_user_password | string | password_hash('sha512') }}"
    email: "{{ email }}"
    groups:
      - admins
      - dev
